      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "new_list = []\n",
    "\n",
    "otra_lista = [\"madrid\", \"barcelona\", \"valencia\"]\n",
    "\n",
    "for i in otra_lista:\n",
    "    new_list.append(i.upper())\n",
    "\n",
    "new_list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "4a31bc76",
   "metadata": {},
